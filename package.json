{
  "name": "release2",
  "version": "0.0.1",
  "description": "Rapidly release projects on the command line",
  "preferGlobal": true,
  "main": "bin/release.js",
  "scripts": {
    "test": "mocha --compilers coffee:coffee-script/register",
    "test-debug": "DEBUG=release* mocha --compilers coffee:coffee-script/register",
    "dev": "mocha --compilers coffee:coffee-script/register --reporter spec --watch"
  },
  "bin": {
    "release": "bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jasonkuhrt/release.git"
  },
  "keywords": [
    "cli",
    "release",
    "semver",
    "version",
    "iterate",
    "automation",
    "utility",
    "deploy",
    "productivity",
    "share",
    "community"
  ],
  "author": "Jason Kuhrt <jasonkuhrt@me.com> (http://jasonkuhrt.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jasonkuhrt/release/issues"
  },
  "homepage": "https://github.com/jasonkuhrt/release",
  "dependencies": {
    "async": "^0.8.0",
    "bluebird": "^2.3.4",
    "commander": "^2.2.0",
    "debug": "^2.1.0",
    "immutable": "^3.0.1",
    "semver": "^2.3.0"
  },
  "devDependencies": {
    "chai": "^1.9.2",
    "coffee-script": "^1.8.0",
    "mocha": "^2.0.1"
  }
}
